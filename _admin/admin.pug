doctype html
html(lang="es")
	head
		
		include ../_pug/_component/head
		
	body
		
		script(src="../js/functions.min.js")
		
		include ../_pug/_component/barraSuperior
		include ../_pug/_component/header
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		link( rel="stylesheet" href="admin.css" )
		
		div.calcular
			h1 calculando venta
			button.agregar agregar
			div.agregando
				input.nombre( type="text" placeholder="Producto" )
				input.precio( type="text" placeholder="Precio" )
				input.talla( type="number" placeholder="Talla" )
				button.listo listo
			
			div.producto
				img( src="data:image/svg+xml;base64," )
				p.nombre NIKE AIR VAPOR MAX
				p.precio( data-precio="23990" ) $ 23.990
				p.talla 42
				select
					option( value="" )
					option( value="dcto15" ) 15% dcto
					option( value="eliminar" ) eliminar
			
			div.producto
				img( src="data:image/svg+xml;base64," )
				p.nombre NIKE AIR VAPOR MAX
				p.precio( data-precio="69990" ) $ 69.990
				p.talla 42
				select
					option( value="" )
					option( value="dcto15" ) 15% dcto
					option( value="eliminar" ) eliminar
			
			div.total
				div.total100 $ 69.990
				div.total50 $ 34.995
		
			script.
				var puntuacion = function( precio ) {
					precio      = String( precio );
					var centena = precio.slice( -3 );
					if ( precio.length <= 3 ) {
						return precio;
					} else if ( precio.length == 4 ) {
						return precio.slice( 0 , 1 ) + "." + centena;
					} else if ( precio.length == 5 ) {
						return precio.slice( 0 , 2 ) + "." + centena;
					} else if ( precio.length == 6 ) {
						return precio.slice( 0 , 3 ) + "." + centena;
					} else if ( precio.length == 7 ) {
						return precio.slice( 0 , 1 ) + "." + precio.slice( 1 , 4 ) + "." + centena;
					} else if ( precio.length == 8 ) {
						return precio.slice( 0 , 2 ) + "." + precio.slice( 2 , 5 ) + "." + centena;
					} else if ( precio.length == 9 ) {
						return precio.slice( 0 , 3 ) + "." + precio.slice( 3 , 6 ) + "." + centena;
					} else {
						return "toi chiquito";
					};
				}
				var sumar = function() {
					var total = 0;
					$.each( $( ".calcular > div.producto" ) , function(){
						var precio = $( this ).find( ".precio" ).attr( "data-precio" );
						total = Number( total ) + Number( precio );
						$( ".calcular .total .total100" ).html( `$ ${ puntuacion( total ) }` );
						$( ".calcular .total .total50" ).html( `$ ${ puntuacion( total / 2 ) }` );
					});
				};sumar();
				var habilitaOption = function() {
					$( ".calcular > div.producto > select" ).change(function(){
						var value = $( this ).val();
						if ( value == "eliminar" ) {
							$( this ).parent().remove();
						} else if ( value == "dcto15" ) {
							let precio = $( this ).parent().find( ".precio" ).attr( "data-precio" );
							$( this ).parent().find( "p.precio" ).html( `$ PENDIENTE!` );
						} else {
							let precio = $( this ).parent().find( ".precio" ).attr( "data-precio" );
							$( this ).parent().find( "p.precio" ).html( `$ ${ puntuacion( precio ) }` );
						};
						sumar();
					});
				};habilitaOption();
		
			script.
				$( ".calcular button.agregar" ).click(function(){
					$( ".calcular > .agregando" ).fadeIn();
				});
				$( ".calcular > .agregando button.listo" ).click(function(){
					var producto = {
						nombre : $( ".calcular > .agregando input.nombre" ).val() ,
						precio : $( ".calcular > .agregando input.precio" ).val() ,
						talla : $( ".calcular > .agregando input.talla" ).val() ,
					};
					$( ".calcular > .agregando" ).after( `
						<div class="producto">
							<img src="data:image/svg+xml;base64,">
							<p class="nombre">${ producto.nombre }</p>
							<p class="precio" data-precio="${ producto.precio }">$ ${ puntuacion( producto.precio ) }</p>
							<p class="talla">${ producto.talla }</p>
							<select>
								<option value=""></option>
								<option value="dcto15">15% dcto</option>
								<option value="eliminar">eliminar</option>
							</select>
						</div>
					` );
					$( ".calcular > .agregando input" ).val( "" );
					$( ".calcular > .agregando" ).hide();
					sumar();
					habilitaOption();
				});
				
			
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		include ../_pug/_component/footer
		