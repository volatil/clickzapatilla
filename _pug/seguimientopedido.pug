doctype html
html(lang="es")
	head
		
		include _component/head
		
	body
		
		script(src="js/functions.min.js")
		
		include _component/analytics
		
		include _component/barraSuperior
		include _component/header
		
		link(rel="stylesheet" href="css/seguimientoPedido.min.css")
		
		.seguimiento
			h1 Seguimiento de Pedido
			
			.buscador
				input( type="text" placeholder="Ingrese número de seguimiento" )
				img.lazyload( data-src="/imagenes/seguimientopedido/lupa.svg" alt="buscar" )
			
			.resultado( style="display:none;" )
				p.cargando
					img.lazyload( data-src="data:image/svg+xml;base64," alt="Buscando ..." )
					| Obteniendo información ...
				div.error
					p Codigo incorrecto
				div.correcto
				script( type="text/javascript" ).
					var getCodigoSeguimiento = function( codigo ){
						Promise.all([
							fetch( `https://spreadsheets.google.com/feeds/list/1aZlC5KaMoyEPVqbMiH8_bPCsZoh65PBW9cm0HpG8Kjk/1/public/values?alt=json` ).then(value => value.json()),
							fetch( `https://spreadsheets.google.com/feeds/list/1aZlC5KaMoyEPVqbMiH8_bPCsZoh65PBW9cm0HpG8Kjk/2/public/values?alt=json` ).then(value => value.json())
						])
						.then((value) => {
							
							var inventario = value[0].feed.entry;
							var ventas = value[1].feed.entry;
							
							for( var fila = 0; fila <= ventas.length-1; fila++ ) {
								
								var codigoSeguimiento = ventas[fila].gsx$idcompra.$t;
								
								if ( String(codigoSeguimiento) == String(codigo) ) {
									
									var idproducto  = ventas[fila].gsx$idproducto.$t;
									var talla       = ventas[fila].gsx$talla.$t;
									var estado      = ventas[fila].gsx$estadopedido.$t;
									var marca       = inventario[idproducto-1].gsx$marca.$t;
									var modelo      = inventario[idproducto-1].gsx$modelo.$t;
									var imagen      = inventario[idproducto-1].gsx$imagen.$t;
									if ( imagen.split( " " ).length >= 2 ) {
										for ( var imgcount = 0; imgcount <= imagen.split( " " ).length-1; imgcount++ ) {
											$( ".getDetalle .galeria" ).append( `<img class="lazyload" src="${imagen.split( " " )[imgcount]}" alt="${marca} - ${modelo}" />` )
											
											$( ".getDetalle > .galeria > img" ).click(function(){
												let ruta = $( this ).attr( "src" );
												$( ".getDetalle > img" ).attr( "src" , ruta );
											});
											
										};
										imagen = imagen.split( " " )[0];
									};
									
									$( ".seguimiento .resultado .correcto" ).html( `
										<div class="compra">
											<img class="lazyload" data-src="${imagen}" alt="${marca} - ${modelo}" />
											<p class="nombre">${marca} - ${modelo}</p>
											<p class="talla">Talla: ${talla}</p>
											<div class="estado">
												<img class="lazyload" data-src="data:image/svg+xml;base64," alt="${estado}" />
												<p>${estado}</p>
											</div>
										</div>
										<div class="historial">
											<ul>
												<li class="estado11 pasado"><span></span>Entregado Satisfactoriamente.</li>
												<li class="estado10 pasado"><span></span>Producto despachado a tu hogar.</li>
												<li class="estado9 pasado"><span></span>Tu producto esta en nuestra Bodega.</li>
												<li class="estado8 pasado"><span></span>Tu producto esta en camino a nuestra bodega.</li>
												<li class="estado7 pasado"><span></span>Tu producto salió de Aduana Chile.</li>
												<li class="estado6 pasado"><span></span>Tu producto fue confirmado en Aeropuerto Chile.</li>
												<li class="estado5 pasado"><span></span>Tu producto se encuentra en Aeropuerto Chile.</li>
												<li class="estado4 pasado"><span></span>Tu producto salió del Aeropuerto con dirección a Chile.</li>
												<li class="estado3 pasado"><span></span>Tu producto fue recibido en el Aeropuerto Internacional.</li>
												<li class="estado2 pasado"><span></span>Tu producto fue despachado al Aeropuerto Internacional.</li>
												<li class="estado1 pasado"><span></span>Obteniendo productos.</li>
											</ul>
										</div>
									` );
									
									switch( estado ) {
											case "Entregado Satisfactoriamente.":
												$( ".historial ul li.estado11" ).addClass( "activo" );
												break;
											case "Producto despachado a tu hogar.":
												$( ".historial ul li.estado10" ).addClass( "activo" );
												break;
											case "Tu producto esta en nuestra Bodega.":
												$( ".historial ul li.estado9" ).addClass( "activo" );
												break;
											case "Tu producto esta en camino a nuestra bodega.":
												$( ".historial ul li.estado8" ).addClass( "activo" );
												break;
											case "Tu producto salió de Aduana Chile.":
												$( ".historial ul li.estado7" ).addClass( "activo" );
												break;
											case "Tu producto fue confirmado en Aeropuerto Chile.":
												$( ".historial ul li.estado6" ).addClass( "activo" );
												break;
											case "Tu producto se encuentra en Aeropuerto Chile.":
												$( ".historial ul li.estado5" ).addClass( "activo" );
												break;
											case "Tu producto salió del Aeropuerto con dirección a Chile.":
												$( ".historial ul li.estado4" ).addClass( "activo" );
												break;
											case "Tu producto fue recibido en el Aeropuerto Internacional.":
												$( ".historial ul li.estado3" ).addClass( "activo" );
												break;
											case "Tu producto fue despachado al Aeropuerto Internacional.":
												$( ".historial ul li.estado2" ).addClass( "activo" );
												break;
											case "Obteniendo productos.":
												$( ".historial ul li.estado1" ).addClass( "activo" );
												break;
											case "Devuelto a Bodega en China":
												$( ".historial ul" ).append( "<li class='estado0 pasado'><span></span>Devuelto a Bodega en China (Nos contactaremos contigo a la brevedad)</li>" );
												$( ".historial ul li.estado0" ).addClass( "activo" );
												break;
											default:
												console.log( `Se perdio el producto uwu` );
									};
									
									for( var contando = 0; contando <= $( ".seguimiento .resultado .correcto > .historial > ul > li" ).length; contando++ ) {
										if ( $( $( ".seguimiento .resultado .correcto > .historial > ul > li" )[contando] ).attr( "class" ).split( " " ).length == 3 ) {
											contando = 100;
										}
										else {
											$( $( ".seguimiento .resultado .correcto > .historial > ul > li" )[contando] ).removeClass( "pasado" )
										}
									}
									
								};
								
							};
							
						});	
					};
					$( ".buscador img" ).click(function(){
						let codidoSeguimiento = $( ".seguimiento > .buscador > input" ).val();
						$( ".seguimiento .resultado" ).show();
						
						if ( codidoSeguimiento.length != 16 ) {
							$( ".seguimiento .resultado .error" ).show();
							$( ".seguimiento .resultado .correcto" ).hide();
						} else {
							getCodigoSeguimiento( codidoSeguimiento );
							$( ".seguimiento .resultado .correcto" ).show();
							$( ".seguimiento .resultado .error" ).hide();
						}
					});
					
					const queryString = window.location.search;
					const urlParams = new URLSearchParams(queryString);
					const codigo = urlParams.get('codigo');
					$( ".seguimiento > .buscador > input" ).val( codigo );
					$( ".seguimiento > .buscador > img" ).click();
					
			.covid
				h3 
					img.lazyload( data-src="data:image/svg+xml;base64," alt="alerta" )
					| COVID-19
				p Estamos siguiendo las recomendaciones internacionales de distanciamiento social y respetando los decretos de Cuarentena y Toque de Queda de las autoridades locales. Esto ha provocado tiempos de respuesta más largos y retrasos en nuestras entregas.
				p Por favor, ten paciencia en este momento tan difícil para todos.
			
		include _component/footer
		